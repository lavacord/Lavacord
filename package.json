{
  "name": "lavacord",
  "version": "2.2.0",
  "description": "A simple by design lavalink wrapper made for any discord library.",
  "exports": {
    ".": "./dist/index.js",
    "./cloudstorm": "./dist/wrappers/cloudstorm.ts",
		"./detritus": "./dist/wrappers/detritus.ts",
		"./discord.js": "./dist/wrappers/discord.js.ts",
		"./eris": "./dist/wrappers/eris.ts",
		"./oceanic": "./dist/wrappers/oceanic.ts"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "postinstall": "yarn compile",
    "prepublishOnly": "yarn compile",
    "lint": "eslint . -c eslint.config.mjs --fix",
    "test": "eslint . -c eslint.config.mjs",
    "compile": "tsc -p .",
    "watch": "tsc -p . -w",
    "docs": "typedoc --plugin typedoc-github-theme"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lavacord/lavacord.git"
  },
  "keywords": [
    "bot",
    "music",
    "discord",
    "api",
    "lava",
    "link",
    "lavalink",
    "discord.js",
    "eris"
  ],
  "author": "Jack Nagle",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lavacord/lavacord/issues"
  },
  "homepage": "https://github.com/MrJacz/lavacord#readme",
  "dependencies": {
    "lavalink-types": "^2.1.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.13.10",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.1.0",
    "typedoc": "^0.28.4",
    "typedoc-github-theme": "^0.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "optionalDependencies": {
    "cloudstorm": "0.13.2",
    "detritus-client": "0.16.x",
    "discord.js": "14.x",
    "eris": "0.18.x",
    "oceanic.js": "1.12.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README"
  ],
  "engines": {
    "node": ">=16.15.0",
    "npm": ">=8"
  }
}
