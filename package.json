  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn compile",
    "lint": "eslint . -c eslint.config.js --fix",
    "test": "eslint . -c eslint.config.js",
